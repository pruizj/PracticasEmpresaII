{
  "name": "@cinema_listings/api",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/server.ts",
    "start-inspect": "node --watch --inspect=0.0.0.0 -r ts-node/register ./src/server.ts",
    "dev-inspect": "yarn run start-inspect",
    "prod": "yarn && yarn run start",
    "generate-types": "graphql-code-generator --config ./codegen.yml",
    "tsc": "tsc --noEmit --p ./tsconfig.json",
    "test-env": "docker-compose -f ./docker-compose.test.yml up",
    "test-env-down": "docker-compose -f ./docker-compose.test.yml down",
    "test": "jest --setupFiles dotenv/config --runInBand",
    "test-inspect": "node --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache --setupFiles dotenv/config"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.18",
    "graphql-upload": "^16.0.2",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.9.2",
    "prettier": "^2.8.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-resolvers": "^3.0.0",
    "@types/jest": "^29.4.0",
    "easygraphql-tester": "^6.0.1",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
